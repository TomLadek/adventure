{
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "node -r dotenv/config node_modules/vite/bin/vite build && npm run deploy",
    "deploy": "node -r dotenv/config ./deploy.js",
    "server": "node -r dotenv/config ./server",
    "server:prod": "cross-env NODE_ENV=production node -r dotenv/config ./server",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@tiptap/extension-placeholder": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "@tiptap/vue-3": "^2.0.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/server-renderer": "^3.2.33",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "express": "^4.18.1",
    "gm": "^1.25.0",
    "gsap": "^3.11.2",
    "mongodb": "^5.2",
    "multer": "^1.4.5-lts.1",
    "photoswipe": "5.3.7",
    "photoswipe-dynamic-caption-plugin": "1.2.7",
    "pinia": "^2.0.21",
    "prettier": "^2.7.1",
    "sirv": "^2.0.2",
    "tippy.js": "^6.3.7",
    "vite": "^4.3.9",
    "vite-plugin-ssr": "^0.4.117",
    "vue": "^3.2.33",
    "vue-i18n": "^9.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  }
}
